system {
    config-management {
        commit-revisions {{ system_commit_revision | default('20') }}
    }
    host-name {{ system_hostname }}
    
    package {
        auto-sync 1
        repository community {
            components main
            distribution helium
            password ""
            url http://packages.vyos.net/vyos
            username ""
        }
    }
    
    time-zone {{system_timezone | default('UTC') }}
}
